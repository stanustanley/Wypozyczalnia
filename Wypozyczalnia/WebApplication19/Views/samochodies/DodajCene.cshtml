@model WebApplication19.Models.cena_wynajmu

@{
    ViewBag.Title = "DodajCene";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<h2>Dodaj cenę</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.samochody_id_samochodu)
        @Html.HiddenFor(model => model.id_cena_wynajmu)

        <div class="form-group">

            <div class="col-md-10">
                Cena wynajmu
                @Html.EditorFor(model => model.cena_wynajmu1, new { htmlAttributes = new { @class = "form-control" } })

            </div>
        </div>

        <div class="form-group">

            <div class="col-md-10">
                Początek obowiązywania ceny
                @Html.EditorFor(model => model.poczatek_obowiazywania_ceny, new { htmlAttributes = new { @class = "form-control" } })

            </div>
        </div>

        <div class="form-group">

            <div class="col-md-10">
                Koniec obowiązywania ceny
                @Html.EditorFor(model => model.koniec_obowiazywania_ceny, new { htmlAttributes = new { @class = "form-control" } })

            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Dodaj" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Wróć do listy", "WidokPracownika_AktualnaCena")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
